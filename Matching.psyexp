        <Param name="Begin Experiment" updates="constant" val="baitL = True&amp;#10;baitR = True&amp;#10;pHi = 0.4&amp;#10;pLo = 0.1&amp;#10;leftHi = random() &gt; 0.5&amp;#10;feedbackDelayMin = 0.5&amp;#10;feedbackDelayMax = 8&amp;#10;feedbackDelayMean = 1.5&amp;#10;totalFish = 0&amp;#10;sessDur = 5*60&amp;#10;timeLeft = sessDur&amp;#10;backDrop = &quot;lake_01.jpg&quot;" valType="extendedCode"/>
