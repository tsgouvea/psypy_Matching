    <Routine name="wait_Sin">
      <CodeComponent name="code_trial">
        <Param name="Begin Experiment" updates="constant" val="from numpy.random import exponential as exprnd" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="corrAns = []&amp;#10;baitL = baitL if baitL else random()&lt;pL&amp;#10;baitR = baitR if baitR else random()&lt;pR&amp;#10;boatOri = 0&amp;#10;if baitL:&amp;#10;    corrAns.append('left')&amp;#10;if baitR:&amp;#10;    corrAns.append('right')&amp;#10;&amp;#10;feedbackDelay = feedbackDelayMin - 1&amp;#10;while (feedbackDelay &lt; feedbackDelayMin) or (feedbackDelay &gt; feedbackDelayMax):&amp;#10;    feedbackDelay = exprnd(feedbackDelayMean)" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
        <Param name="Each Frame" updates="constant" val="timeLeft = max(0,sessDur-globalClock.getTime())" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="if resp.keys not in corrAns:#stored on last run routine&amp;#10;  feedbackDelay = 60*60" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="code_trial" valType="code"/>
      </CodeComponent>
      <KeyboardComponent name="resp">
        <Param name="allowedKeys" updates="constant" val="'left', 'right'" valType="code"/>
